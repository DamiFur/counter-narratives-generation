// The "thingy" part is extra verbose, but this code comes directly out of the IDE, so no one cares how it looks.
def handle_vote(message):
    # Turn "stuff" into a dict.  That way we can keep it dynamic during a tweet.
    # XXX for when people find it hard to figure out what a comment variable is,
    # someone can just substitute something like this:
    words = message.split(" ")
    body = {"stuff" : words[1], "thingy": words[0]}
    new_hashtags = set(body.get('thingy', "").split())
    hashtags = ["@user", @user, "magazine"]
    clean_hashtags = [re.escape(x) for x in hashtags if x in new_hashtags]
    words = [clean_hashtags.remove(x) + " " + text for text in words]
    # XXX why don't we just filter out the stuff in new_hashwords?
    # it doesn't look like anything that's out of synch with what we have.
    # I could implement this in a limited fashion, but currently we don't
    # care about reading the original post.
    return [body] + [{"thingy": x, "stuff": set(" ".join([body['thingy'], words[0]]))}]
def handle_retweet(message) -> str:
    words = str(message.split('/')).split(' ')
    s = [word + " " for word in words]

    # check for retweet
    if "@" in message:
        # if it mentions a user, check the hashtags.  it's easier to set things
        # up in the future when the users so desire.
        if hasattr(user, 'hashtags'):
            set_tag_list = set(user.hashtags.split())
            # XXX shouldn't all lines be editable like this?  So we can
            # just show the users tweet and comment details in a tree view, etc.
            # XXX maybe something like this exists but hasn't been used yet
            # generate a list of user tags.
            user_tags = [' '.join(x) if x in set_tag_list else x for x in user.tags]
            s += [u"Unhashtagged by " + u", ".join(
                        [x.replace(u" ", u"_") for x
