{'bleu': 0.0, 'precisions': [0.012867647058823529, 0.0, 0.0, 0.0], 'brevity_penalty': 1.0, 'length_ratio': 27.2, 'translation_length': 544, 'reference_length': 20}
{'rouge1': 0.03149606299212598, 'rouge2': 0.0, 'rougeL': 0.023622047244094484, 'rougeLsum': 0.023622047244094484}
