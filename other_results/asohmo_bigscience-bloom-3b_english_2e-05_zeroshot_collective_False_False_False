{'bleu': 0.0, 'precisions': [0.011782032400589101, 0.0, 0.0, 0.0], 'brevity_penalty': 1.0, 'length_ratio': 33.95, 'translation_length': 679, 'reference_length': 20}
{'rouge1': 0.03205128205128205, 'rouge2': 0.0, 'rougeL': 0.025641025641025644, 'rougeLsum': 0.03205128205128205}
