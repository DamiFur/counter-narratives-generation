#!/usr/bin/env python
# coding: utf-8

import re

def non_hateful(message):
    if re.search(r"^(@|#)[a-z]+([a-z][a-z0-9]+)*\b", message):
        return message
    else:
        return message + " " + non_hateful

if __name__ == '__main__':
    message = "Refusing to send migrants back to Italy"
    message = non_hateful + message
    message = message.strip()
    print message
    print message.replace("@", "#")
    print message.strip()</s>
